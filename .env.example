# Telegram Bot Configuration
# Получите токен у @BotFather в Telegram
BOT_TOKEN=your_bot_token_here

# Admin Configuration
# Ваш Telegram ID (узнайте у @userinfobot)
ADMIN_CHAT_ID=your_telegram_id_here

# Timezone
# Таймзона для отображения времени
TZ=Europe/Moscow

# Google Calendar Integration (Опционально)
# Если не заполните - бот будет работать без календаря

# ID календаря из настроек Google Calendar
GOOGLE_CALENDAR_ID=primary

# JSON credentials от Google Service Account
# ВАЖНО: Должен быть в одну строку!
GOOGLE_CALENDAR_CREDENTIALS=
